{% extends 'base-admin.html.twig' %}

{% block title %}Command{% endblock %}

{% block body %}
    <h2>Commandes</h2>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ command.id }}</td>
            </tr>
            <tr>
                <th>TotalPrice</th>
                <td>{{ command.totalPrice }}</td>
            </tr>
            <tr>
                <th>NumCommand</th>
                <td>{{ command.numCommand }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ command.createdAt ? command.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ command.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_command_index') }}">back to list</a>

    {# <a href="{{ path('app_command_edit', {'id': command.id}) }}">edit</a>

    {{ include('command/_delete_form.html.twig') }} #}
{% endblock %}
